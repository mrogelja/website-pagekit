#: extensions/installer/src/Pagekit/Installer/Controller/InstallerController.php:18
#: extensions/installer/theme/templates/template.blade.php:5
msgid "Pagekit Installer"
msgstr "קובץ התקנת פייג' קיט"

#: extensions/installer/src/Pagekit/Installer/Controller/InstallerController.php:46
msgid "Database connection failed!"
msgstr "ההתחברות למאגר הנתונים נכשלה!"

#: extensions/installer/src/Pagekit/Installer/Controller/InstallerController.php:66
msgid "No database connection."
msgstr "אין חיבור למאגר הנתונים."

#: extensions/installer/src/Pagekit/Installer/Controller/InstallerController.php:112
msgid "Can't write config."
msgstr "אין אפשרות לרשום בקובץ ההגדרות."

#: extensions/installer/src/Pagekit/Installer/Controller/InstallerController.php:125
msgid "DB user is missing required privileges."
msgstr "משתמש מאגר הנתונים לא נמצא או שאין לו את הפריבליגות הנדרשות."

#: extensions/installer/views/install.blade.php:13
msgid "Begin"
msgstr "התחלה"

#: extensions/installer/views/install.blade.php:23
msgid "Connect database"
msgstr "התחבר למאגר הנתונים."

#: extensions/installer/views/install.blade.php:25
msgid "Enter your database connection details."
msgstr "הזן את פרטי ההתחברות למאגר הנתונים."

#: extensions/installer/views/install.blade.php:30
msgid "Hostname"
msgstr "שרת מארח"

#: extensions/installer/views/install.blade.php:33
msgid "Host cannot be blank."
msgstr "יש למלא את את השדה: שרת מארח"

#: extensions/installer/views/install.blade.php:41
msgid "User cannot be blank."
msgstr "יש למלא את השדה: משתמש."

#: extensions/installer/views/install.blade.php:51
msgid "Database Name"
msgstr "שם מאגר הנתונים"

#: extensions/installer/views/install.blade.php:54
msgid "Database name cannot be blank."
msgstr "יש למלא את השדה: שם מאגר הנתונים."

#: extensions/installer/views/install.blade.php:59
msgid "Table Prefix"
msgstr "פתיח טבלה במאגר הנתונים."

#: extensions/installer/views/install.blade.php:64
#: extensions/installer/views/install.blade.php:107
#: extensions/installer/views/install.blade.php:140
msgid "Next"
msgstr "הבא"

#: extensions/installer/views/install.blade.php:75
msgid "Create your account"
msgstr "יצירת חשבון."

#: extensions/installer/views/install.blade.php:77
msgid "You will be the site administrator."
msgstr "אתה תהייה מנהל האתר."

#: extensions/installer/views/install.blade.php:85
msgid "Username cannot be blank."
msgstr "יש למלא את השדה: שם משתמש."

#: extensions/installer/views/install.blade.php:93
msgid "Field must be a valid email address."
msgstr "יש למלא כתובת אימייל תקנית."

#: extensions/installer/views/install.blade.php:102
msgid "Password cannot be blank."
msgstr "יש למלא את השדה: סיסמה."

#: extensions/installer/views/install.blade.php:118
msgid "Setup your site"
msgstr "הגדרות האתר."

#: extensions/installer/views/install.blade.php:120
msgid "Enter your website details."
msgstr "הזנת פרטי האתר."

#: extensions/installer/views/install.blade.php:128
msgid "Site name cannot be blank."
msgstr "יש למלא את השדה: שם האתר."

#: extensions/installer/views/install.blade.php:152
msgid "Installing Pagekit..."
msgstr "מתקין את פייג' קיט..."

#: extensions/installer/views/install.blade.php:160
msgid "Installation failed!"
msgstr "ההתקנה נכשלה!"

#: extensions/installer/views/install.blade.php:169
msgid "Retry"
msgstr "נסיון חוזר"

#: extensions/installer/views/install.blade.php:175
msgid "Finished!"
msgstr "ההתקנה הושלמה!"

#: extensions/installer/views/install.blade.php:182
msgid "Login now"
msgstr "כניסה למערכת."
