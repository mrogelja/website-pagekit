msgstr "Introduce los detalles del sitio."

#: extensions/installer/views/install.blade.php:128
msgid "Site name cannot be blank."
msgstr "El nombre del sitio no puede estar vacío."

#: extensions/installer/views/install.blade.php:152
msgid "Installing Pagekit..."
msgstr "Instalando Pagekit..."

#: extensions/installer/views/install.blade.php:160
msgid "Installation failed!"
msgstr "¡Instalación fallida!"

#: extensions/installer/views/install.blade.php:169
msgid "Retry"
msgstr "Reintentar"

#: extensions/installer/views/install.blade.php:175
msgid "Finished!"
msgstr "¡Finalizado!"

#: extensions/installer/views/install.blade.php:182
