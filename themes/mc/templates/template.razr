                        <nav class="uk-navbar">
                            <div class="uk-hidden-small">
                                @section('navbar', ['renderer' => 'navbar'])
                            </div>
                            <div class="uk-navbar-flip">
                                <ul class="uk-navbar-nav">
                                    <li>
                                        <a href="#offcanvas" class="uk-hidden-small" data-uk-offcanvas> <i class="uk-icon-key"></i> </a>
